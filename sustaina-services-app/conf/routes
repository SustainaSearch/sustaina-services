
# Catalog
GET         /api/v1/catalog         com.sustainasearch.services.catalog.CatalogController.query(q: String, rows: Long, fuzzy: Boolean, sp: Option[String])

# Produtcs
GET         /api/v1/catalog/products        com.sustainasearch.services.catalog.products.ProductsController.query(q: String, start: Long, rows: Long, fuzzy: Boolean, sp: Option[String])
POST        /api/v1/catalog/products        com.sustainasearch.services.catalog.products.ProductsController.add()

# Swagger
GET         /api-doc                controllers.Assets.at(path="/public/api-doc", file="/index.html")
GET         /api-doc/               controllers.Assets.at(path="/public/api-doc", file="/index.html")
GET         /api-doc/*file          controllers.Assets.at(path="/public/api-doc", file)
GET         /swagger.json           controllers.ApiHelpController.getResources